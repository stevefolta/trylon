Class: CompiledField

name: String;
type: CompiledClass;
is-primitive: Bool;
initial-value: Expression; 	// Only used for class/package fields.
captured-index: Int;	// > 0 if used in a lambda


create(name: String, type: CompiledClass)
{
	this.name = name;
	this.type = type;
	this.is-primitive = false;
}


create(name: String, type: CompiledClass, is-primitive: Bool)
{
	this.name = name;
	this.type = type;
	this.is-primitive = is-primitive;
}


