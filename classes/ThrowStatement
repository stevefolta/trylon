Class: ThrowStatement
Superclass: Statement


value: Expression;


create(value: Expression, comment: String)
{
	this.value = value;
	this.comment = comment;
}


prepare-to-emit()
{
	value.prepare-to-emit();
}


emit-code(builder: MethodBuilder)
{
	result: ExpressionResult = value.emit-code(builder);
	builder.add-line(add-comment("Throw_(" + result.access + ");"));
}


