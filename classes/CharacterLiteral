Class: CharacterLiteral
Superclass: Expression

name: String;


create(name: String)
{
	this.name = name;
}


prepare-to-emit()
{
	// Nothing to do.
}


emit-code(builder: MethodBuilder): ExpressionResult
{
	mangled-name: String = MethodBuilder.mangle-name(name);
	builder.add-extern-declaration("UsingChar_(" + mangled-name + ")");
	return new NameResult("Char_(" + mangled-name + ")");
}


