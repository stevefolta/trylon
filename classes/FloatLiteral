Class: FloatLiteral
Superclass: Expression


value: Float;
index: Int; 	// The MethodBuilder will set this.


create(value: String)
{
	this.value = value.as-float;
}


emit-code(builder: MethodBuilder): ExpressionResult
{
	builder.add-float-literal(this); 	// the "index" gets set here
	return new FloatLiteralResult(index);
}


