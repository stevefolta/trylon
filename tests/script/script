#!/usr/bin/env trylon

trylon Script

main: args
	send: "Hello world!"
	args pop-front
	if !args is-empty
		send: "Args:"
		for arg in args
			send: ("  ", arg)
		send: "-----"
	foo
	Foo foo


foo
	for item in ("One", "Two", "Three")
		send: item

class Foo
	foo
		for item in ("Four", "Five", "Six")
			send: item
