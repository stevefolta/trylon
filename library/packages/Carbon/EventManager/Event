trylon-class Event

field primitive-event 	# Don't touch this.


primitive-fn create: event-class kind: kind
primitive-fn release

primitive-fn type -> Symbol 	--
	# Our special combo of class & kind as a handy Symbol

primitive-fn event-class -> Int
primitive-fn kind -> Int

primitive-fn uint-parameter: parameter [Int] -> Int
primitive-fn point-parameter: parameter [Int] -> Point
primitive-fn rect-parameter: parameter [Int] -> Rectangle
primitive-fn mouse-button-parameter: parameter -> Symbol
primitive-fn unicode-text-parameter: parameter -> String


primitive-fn target: target

