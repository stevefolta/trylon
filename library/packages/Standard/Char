class Char

	class-fn new
		return --
			Implementation allocate-object class: (Char) with-extra-slots: 1

	primitive-fn == arg [Char] -> Bool
	primitive-fn != arg [Char] -> Bool
	primitive-fn < arg [Char] -> Bool
	primitive-fn > arg [Char] -> Bool
	primitive-fn <= arg [Char] -> Bool
	primitive-fn >= arg [Char] -> Bool

	primitive-fn + arg [Char] -> Char
	primitive-fn - arg [Char] -> Char

	primitive-fn string -> String
