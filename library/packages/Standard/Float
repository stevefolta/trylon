trylon-class Float

primitive-fn == arg [Float] -> Bool
primitive-fn != arg [Float] -> Bool
primitive-fn < arg [Float] -> Bool
primitive-fn > arg [Float] -> Bool
primitive-fn <= arg [Float] -> Bool
primitive-fn >= arg [Float] -> Bool

primitive-fn + arg [Float] -> Float
primitive-fn - arg [Float] -> Float
primitive-fn * arg [Float] -> Float
primitive-fn / arg [Float] -> Float
primitive-fn unary-minus -> Float

primitive-fn pow: power [Float] -> Float
primitive-fn sqrt -> Float

primitive-fn string -> String
primitive-fn int -> Int


iff debugger
	fn debug-write: level
		indent := debug-spaces substr: 0 length: level
		print-line: indent + this string

