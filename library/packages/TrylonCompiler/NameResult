trylon NameResult
superclass ExpressionResult

# Used for various very simple ExpressionResults, which only have to reference 
# a name.

fields name declared-type is-constant


create: name
	create: name is-constant: false


create: name is-constant: is-constant
	this name = name
	this is-constant = is-constant


create: name type: type
	this name = name
	this declared-type = type
	this is-constant = false


access
	return name

type
	return declared-type




