trylon CharacterLiteral
superclass Expression

fields name


create: name
	this name = name


prepare-to-emit
	# Nothing to do.


emit-code: builder
	mangled-name = mangle-name: name
	builder add-extern-declaration: "UsingChar_(" + mangled-name + ")"
	return NameResult new: "Char_(" + mangled-name + ")"


emit-package-literal: stream
	mangled-name = mangle-name: name

	stream write: "UsingChar_"
	stream write: mangled-name
	stream write: ")"
	stream write-line

	return NameResult new: "Char_(" + mangled-name + ")"



