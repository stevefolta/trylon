trylid Int

# Just about all of these are primitives defined elsewhere.

fn == arg (Int) -> Bool
fn != arg (Int) -> Bool
fn < arg (Int) -> Bool
fn > arg (Int) -> Bool
fn <= arg (Int) -> Bool
fn >= arg (Int) -> Bool

fn + arg (Int) -> Int
fn - arg (Int) -> Int
fn * arg (Int) -> Int
fn / arg (Int) -> Int
fn % arg (Int) -> Int
fn unary-minus -> Int

fn | arg (Int) -> Int
fn & arg (Int) -> Int
fn ^ arg (Int) -> Int
fn ~ -> Int
fn << arg (Int) -> Int
fn >> arg (Int) -> Int

fn string
fn float
fn char



iff debugger
	fn debug-write
		print-line: "Int: " + this string



