trylon Int

# Just about all of these are primitives defined elsewhere.

fn == arg (Int)
fn != arg (Int)
fn < arg (Int)
fn > arg (Int)
fn <= arg (Int)
fn >= arg (Int)

fn + arg (Int)
fn - arg (Int)
fn * arg (Int)
fn / arg (Int)
fn % arg (Int)
fn unary-minus

fn | arg (Int)
fn & arg (Int)
fn ^ arg (Int)
fn ~
fn << arg (Int)
fn >> arg (Int)

fn string
fn char-string
fn float
fn is-digit
fn is-whitespace

# For compatibility with Trylon 1 code, which has Int and Char as separate
# classes.
char
	return this
int
	return this



debug-write
	print-line: "Int: " + this string



