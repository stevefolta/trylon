trylon Object

extend Standard Object
	c-fn raw-new
		return CloneObj_(this_);
	
	c-fn object-ptr
		return BuildBytePtr_((byte_ptr_) this_);
	
	c-fn same-as: other
		return Bool_(this_ == other);
	
	c-fn .proto
		return this_->class_->proto;
	
	c-fn .parent-proto
		return this_->class_->superclass;


