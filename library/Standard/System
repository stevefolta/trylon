trylid System


primitive-fn milliseconds
	etc.
primitive-fn sleep-ms: milliseconds
	etc.

primitive-fn num-garbage-collections
	etc.
primitive-fn dump-gc-stats
	etc.

debugger
	coke
		(let ((getpid (dlsym "getpid"))
		      (kill (dlsym "kill")))
			(kill (getpid) 5))

environment-variable-named: name
	coke
		(let ((value (getenv [name _stringValue])))
			(if value [(trylid-proto Standard String) new_: value] nil))

primitive-fn current-directory-path
	etc.
primitive-fn current-directory-path: new-path
	etc.
primitive-fn run-program: name arguments: arguments
	etc.
primitive-fn sysname
	etc.
	return ""
primitive-fn strftime: time-int format: format
	etc.
	return "!strftime not implemented!"


