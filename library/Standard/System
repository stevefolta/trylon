# These are all primitives, defined elsewhere.

milliseconds
sleep-ms: milliseconds
num-garbage-collections
dump-gc-stats

debugger

environment-variable-named: name

current-directory-path
current-directory-path: new-path

sysname
strftime: time-int format: format

run-program: name arguments: arguments
run-program: name with: arguments
	run-program: name arguments: arguments
run: program with: arguments
	run-program: program arguments: arguments
run: program
	run-program: program arguments: nil


