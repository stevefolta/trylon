trylon VoidFunction

c-fn call: name with: args
	ffi_arg result;
	CallFFI_(name, args, &ffi_type_void, &result);


new: name
	return NamedFunction new: name

class NamedFunction
	superclass VoidFunction
	fields name

	create: name
		this name = name
	
	call: args
		call: name with: args


c-preamble
	#include <ffi.h>

	extern void CallFFI_(obj_ fn_name, obj_ args, ffi_type* result_type, void* result);


