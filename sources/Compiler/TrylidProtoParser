trylon TrylidProtoParser

fields lines proto context


create: lines into: proto context: context
	this lines = lines
	this proto = proto
	this context = context


parse
	parse-block: lines


parse-block: lines
	for line in lines
		parse-line: line


parse-line: line
	words = line words
	switch words peek
		"proto", "class"
			#...
		"fn", "function", "class-fn"
			#...
		"field", "fields"
			#...
		"extend"
			#...
		"trylid", "trylon"
			#...
		else
			#...



