trylon PrimitiveFunction
superclass TrylonFunction

# Functions that are defined outside Trylon code.
# Will only exist inside a method, not as part of a Class.

fields name on-proto


create: name on-proto: on-proto
	this name = name
	this on-proto = on-proto


compile-call: builder with: arguments
	return builder compile-primitive-function-call: this with: arguments



