trylon BoolLiteral
superclass TrylidExpression

fields value


create: value
	this value = value


prepare-to-emit
	# Nothing to do.


jolt-expression
	if value
		return 'true'
	else
		return 'false'


translate: compiler
	return compiler translateExpression: jolt-expression


