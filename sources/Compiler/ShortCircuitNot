trylon ShortCircuitNot
superclass TrylidExpression

fields expr


create: expr
	this expr = expr


prepare-to-emit
	expr prepare-to-emit


jolt-expression
	return Expression with: 'not' with: expr jolt-expression


translate: compiler
	expr = Expression with: 'not' with: expr
	return compiler translateExpression: expr



