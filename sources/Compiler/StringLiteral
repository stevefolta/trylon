trylon StringLiteral
superclass TrylidExpression

field value


create: value
	this value = value


prepare-to-emit
	# Nothing to do.


jolt-expression
	return Expression with: 'trylid-string-literal' with: value


translate: compiler
	return compiler translateExpression: (Expression with: 'quote' with: value)


