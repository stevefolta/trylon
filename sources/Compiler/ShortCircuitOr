trylon ShortCircuitOr
superclass TrylonExpression

fields expr1 expr2


create: expr1 with: expr2
	this expr1 = expr1
	this expr2 = expr2


prepare-to-emit
	expr1 prepare-to-emit
	expr2 prepare-to-emit


compile: builder
	return builder compile-short-circuit-or: this




