trylon IntLiteral
superclass TrylidExpression

fields value


create: value
	this value = value int


prepare-to-emit
	# Nothing to do.


jolt-expression
	return Expression with: 'trylid-int-literal' with: value


translate: compiler
	return compiler translateExpression: (Expression with: 'quote' with: value)


