trylon CharacterLiteral
superclass TrylidExpression

fields char


create: char
	this char = char


prepare-to-emit
	# Nothing to do.


jolt-expression
	return Expression with: 'trylid-char-literal' with: char int


translate: compiler
	return compiler translateExpression: (Expression with: 'quote' with: char)



