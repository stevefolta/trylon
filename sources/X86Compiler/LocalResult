trylon LocalResult
superclass Result

fields offset

create: local
	# Is this correct, or does the offset need an offset?  Or a negation?
	offset = local compile-info index * word-size


mod-rm
	if offset > 127
		# ...
	else
		return 0x75


disp8
	if offset <= 127
		return offset
	return nil

disp32
	if offset > 127
		return offset
	return nil


