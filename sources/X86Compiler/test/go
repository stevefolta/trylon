#!/usr/bin/env trylon

source = File contents-of: "replacement-code"
X86Compiler 	--
	replace-method: 'say-it' on: X86Compiler test with: source arg-names: empty

X86Compiler test please-say-it

send: ""

send: "Timing..."
BenchObject = X86Compiler test BenchObject
arg-names = List with: "n"
source = File contents-of: "nfibs"
X86Compiler 	--
	replace-method: 'nfibs:' on: BenchObject with: source arg-names: arg-names
X86Compiler test time-nfibs

