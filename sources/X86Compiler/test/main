trylon test


say-it
	send: "Replacement failed."

please-say-it
	result = say-it
	if result
		send: ("Returned:  ", result)


test: value against: expected-value name: name
	if value == expected-value
		send: (name, ":  succeeded.")
	else
		send: (name, ":  FAILED.")

check: ok name: name
	if ok
		send: (name, ":  succeeded.")
	else
		send: (name, ":  FAILED.")

