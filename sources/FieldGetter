trylon FieldGetter
superclass TrylidFunction


fields field on-proto


create: field on-proto: on-proto
	this field = field
	this on-proto = on-proto


emit-call: arguments
	# Could do this more efficiently...
	return 	--
		Expression 	--
			with: 'send' 	--
			with: (Expression with: 'quote' with: field name intern) 	--
			with: on-proto access


name
	return field name



